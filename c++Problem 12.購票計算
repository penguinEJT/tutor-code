#include <iostream>
using namespace std;

int main() {
    int n;
    int nt10, nt5, nt1;
    cin >> n;

    nt10 = n / 10;
    n = n % 10;

    nt5 = n / 5;
    n = n % 5;

    nt1 = n;

    cout << "NT10=" << nt10 << endl;
    cout << "NT5=" << nt5 << endl;
    cout << "NT1=" << nt1 << endl;

    return 0;
}
