#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>
int main(){
    int n;
    scanf("%d",&n);
    int a[n][2];
    for(int i=0;i<n;i++){
        scanf("%d %d",&a[i][0],&a[i][1]);
    }
    for(int i=0;i<n;i++){
        a[i][0]=a[i][0]+a[i][1];
        a[i][0]=a[i][0]*a[i][0];
        printf("%d\n",a[i][0]);
    }
    return 0;
}
