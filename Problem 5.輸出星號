#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(){
    char ch[10];
    for(int i=0;i<5;i++){
        scanf("%c",&ch[i]);
    }
    for(int i=0;i<5;i++){
        for(int j=0;j<ch[i]-'0';j++){   \\要用整數值
             printf("*");
        }  
        printf("\n");
    }    
    return 0;
}
