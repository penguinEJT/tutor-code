#include <stdio.h>
#include <string.h>
int main(){
  int number;
  int a[100];
  int start;
  int end;
  int sum=0;
  scanf("%d",&number);
  for(int i=0;i<number;i++){
    scanf("%d",&a[i]);
  }
  scanf("%d %d",&start,&end);
  for(int i=start;i<=end;i++){
    sum=sum+a[i-1];
  }
  printf("%d",sum);
  printf("\n");
return 0;
}
