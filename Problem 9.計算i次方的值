#include <stdio.h>
#include <math.h>

int main() {
    int n;  
    scanf("%d", &n);  
    for (int j = 0; j < n; j++) {
        int i;
        scanf("%d", &i); 
        if (i > 31) {
            printf("Value of more than 31\n");
        } else {
            // 使用位移運算計算 2^i
            printf("%lld\n", (long long)1 << i);
        }
    }
    
    return 0;
}
