#include <iostream>

int main() {
    int choice;
    std::cin >> choice;
    switch (choice) {
        case 1:
            std::cout << "Person" << std::endl;
            break;
        case 2:
            std::cout << "Fairy" << std::endl;
            break;
        case 3:
            std::cout << "Dwarf" << std::endl;
            break;
        default:
            std::cout << "Invalid choice" << std::endl;
    }
    return 0;
}
