#include <stdio.h>
#include <math.h>
    int main(){
  int x,t=0;
  scanf("%d",&x);
  if(x==1){
    printf("NO\n");
    return 0;
  }
  int y=ceil(sqrt(x));
  for(int i=2;i<=y;i++){
    if(x%i==0){
        t=i; 
    }
  }
  if(t==0){
    printf("YES\n");
  }
  else{
    printf("NO\n");
  }
  return 0;
}
