#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>
int main(){
    int n;
    scanf("%d",&n);
    double a[n];
    for(int i=0;i<n;i++){
        scanf("%lf",&a[i]);
    }
    for(int i=0;i<n;i++){
        a[i]=a[i]*a[i];
        a[i]=round(10*a[i])/10;
        printf("%.1f\n",a[i]);
    }
    return 0;
}
