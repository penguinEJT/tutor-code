#include <iostream>

int main() {
    int number, a, b, c, d;
    std::cin >> number;

    a = number / 1000;
    number = number - 1000 * a;
    b = number / 100;
    number = number - 100 * b;
    c = number / 10;
    number = number - 10 * c;
    d = number;

    std::cout << d << "," << c << "," << b << "," << a << std::endl;

    return 0;
}
