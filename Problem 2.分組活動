#include <stdio.h>
#include <string.h>
int main(){
   int number;
   char a[11];
   int group;
   scanf("%d", &number);
   scanf("%s", a);
   scanf("%d", &group);
   for(int i=0;i<number;i++){
     printf("%c", a[i]);
       if((i+1) % group==0 && (i+1)!=number){
         printf(" "); 
      }
   }
printf("\n");
return 0;
}
